package örnekler;

public class örnekler {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=2,y=3;
		System.out.println("i is ");
		if(i>2)
			if(y>2) {
				int z=i+y;
				System.out.println("z is "+z);
			}
				
			else
				System.out.println("i is "+i);

	}

}
