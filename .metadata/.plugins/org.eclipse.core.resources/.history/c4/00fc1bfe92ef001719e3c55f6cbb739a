package örnekler;
import java.util.*;
public class örnekler {

	public static void main(String [] args) {
		int num=0;
		int [] numbers=new int[1];
		m(num,numbers);
		System.out.println("number is "+ num+"and numbers[0] is "+ numbers[0]);
	}
	
	public static void m(int x,int []y) {
		x=3;
		y[0]=3;
	}
}
